<div style="display: flex; gap: 20px">
  {{#each response}}
    <div style="width: 30%;">
      <div style="display: grid; place-content: center;">
        <img
          src="{{thumbnails}}"
          alt="{{title}}"
          style="aspect-ratio: 1; width: 200px; object-fit: cover;"
        />
      </div>
      <h2>{{title}}</h2>
      <h2>{{price}}</h2>
      <p>{{description}}</p>
    </div>
  {{/each}}
</div>